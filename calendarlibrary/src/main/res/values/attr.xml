        <attr name="iRLDoorDrawable" format="reference" /><!--后左-->
        <attr name="iRRDoorDrawable" format="reference" /><!--后右-->
        <attr name="iFDoorDrawable" format="reference" /><!--引擎盖-->
        <attr name="iRDoorDrawable" format="reference" /><!--后尾箱-->
